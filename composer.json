{
  "name": "fluxlabs/fluxmailapi",
  "description": "Fetch or send mails",
  "type": "project",
  "keywords": [
    "fetch",
    "mail",
    "send"
  ],
  "homepage": "https://github.com/fluxapps/FluxMailApi",
  "license": "GPL-3.0-only",
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-imap": "*",
    "ext-swoole": "*",
    "phpmailer/phpmailer": "6.5.0",
    "php-imap/php-imap": "4.1.0"
  },
  "autoload": {
    "psr-4": {
      "Fluxlabs\\FluxMailApi\\": "src/"
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
